{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "artesaos/seotools": "^1.2",
        "blade-ui-kit/blade-heroicons": "^2.3.0",
        "filament/filament": "^3.2.34",
        "filament/spatie-laravel-google-fonts-plugin": "^3.2.34",
        "filament/spatie-laravel-media-library-plugin": "^3.2.34",
        "filament/spatie-laravel-translatable-plugin": "^3.2.34",
        "foxws/livewire-use": "^0.0.10",
        "guzzlehttp/guzzle": "^7.8.1",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/framework": "^10.44.0",
        "laravel/horizon": "^5.23.0",
        "laravel/sanctum": "^3.3.3",
        "laravel/scout": "^10.8.3",
        "laravel/telescope": "^4.17.6",
        "laravel/tinker": "^2.9.0",
        "livewire/livewire": "^3.4.4",
        "meilisearch/meilisearch-php": "^1.6.1",
        "php-ffmpeg/php-ffmpeg": "^1.2",
        "pusher/pusher-php-server": "^7.2.4",
        "spatie/laravel-activitylog": "^4.7.3",
        "spatie/laravel-collection-macros": "^7.13.1",
        "spatie/laravel-db-snapshots": "^2.5.2",
        "spatie/laravel-enum": "^3.0.2",
        "spatie/laravel-google-fonts": "^1.2.3",
        "spatie/laravel-medialibrary": "^11.4.1",
        "spatie/laravel-model-states": "^2.6.2",
        "spatie/laravel-permission": "^6.3",
        "spatie/laravel-prefixed-ids": "^1.3.1",
        "spatie/laravel-responsecache": "^7.4.10",
        "spatie/laravel-sluggable": "^3.5.0",
        "spatie/laravel-tags": "^4.5.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.15.1",
        "fakerphp/faker": "^1.23.1",
        "laravel/pint": "^1.13.11",
        "laravel/sail": "^1.27.4",
        "mockery/mockery": "^1.6.7",
        "nunomaduro/collision": "^7.10.0",
        "pestphp/pest": "^2.34.0",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpunit/phpunit": "^10.5.10",
        "spatie/laravel-ignition": "^2.4.2"
    },
    "autoload": {
        "files": [
            "src/Foundation/Helpers.php"
        ],
        "psr-4": {
            "App\\": "src/App/",
            "Domain\\": "src/Domain/",
            "Foundation\\": "src/Foundation/",
            "Support\\": "src/Support/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

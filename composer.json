{
    "name": "francoism90/hub",
    "type": "project",
    "description": "A video on demand (VOD) media distribution system.",
    "keywords": [
        "framework",
        "laravel",
        "nginx-vod-module",
        "ngx-vod-module",
        "vod",
        "video-on-demand",
        "media"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "artesaos/seotools": "^1.3.2",
        "blade-ui-kit/blade-heroicons": "^2.6.0",
        "foxws/laravel-algos": "^0.2.0",
        "foxws/laravel-modelcache": "^1.1.0",
        "foxws/wireuse": "^2.9.6",
        "guzzlehttp/guzzle": "^7.9.3",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/framework": "^12.17.0",
        "laravel/horizon": "^5.32.1",
        "laravel/octane": "^2.9.3",
        "laravel/prompts": "^0.3.5",
        "laravel/reverb": "^1.5.0",
        "laravel/sanctum": "^4.1.1",
        "laravel/scout": "^10.15.0",
        "laravel/telescope": "^5.9.0",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.29",
        "league/flysystem-read-only": "^3.28",
        "livewire/livewire": "^3.6.3",
        "meilisearch/meilisearch-php": "^1.14.0",
        "php-ffmpeg/php-ffmpeg": "^1.3.2",
        "pusher/pusher-php-server": "^7.2.7",
        "spatie/eloquent-sortable": "^4.5.0",
        "spatie/laravel-collection-macros": "^8.0",
        "spatie/laravel-flash": "^1.10.1",
        "spatie/laravel-google-fonts": "^1.4.3",
        "spatie/laravel-html": "^3.12.0",
        "spatie/laravel-medialibrary": "^11.13.0",
        "spatie/laravel-model-states": "^2.11.3",
        "spatie/laravel-permission": "^6.19.0",
        "spatie/laravel-prefixed-ids": "^1.4.1",
        "spatie/laravel-responsecache": "^7.7.0",
        "spatie/laravel-sluggable": "^3.7.5",
        "spatie/laravel-tags": "^4.10.0",
        "spatie/php-structure-discoverer": "^2.3.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5.5",
        "fakerphp/faker": "^1.24.1",
        "laravel/envoy": "^2.10.2",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.22.1",
        "laravel/sail": "^1.43.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.0",
        "pestphp/pest": "^3.8.2",
        "pestphp/pest-plugin-laravel": "^3.2.0",
        "phpunit/phpunit": "^11.5.15"
    },
    "autoload": {
        "files": [
            "src/Foundation/Helpers.php"
        ],
        "psr-4": {
            "App\\": "src/App/",
            "Domain\\": "src/Domain/",
            "Foundation\\": "src/Foundation/",
            "Support\\": "src/Support/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

{
    "name": "francoism90/hub",
    "type": "project",
    "description": "A video on demand (VOD) media distribution system.",
    "keywords": [
        "framework",
        "laravel",
        "nginx-vod-module",
        "ngx-vod-module",
        "vod",
        "video-on-demand",
        "media"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.9.3",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^2.0.3",
        "laravel/fortify": "^1.27.0",
        "laravel/framework": "^12.19.3",
        "laravel/horizon": "^5.33.1",
        "laravel/octane": "^2.10.0",
        "laravel/prompts": "^0.3.5",
        "laravel/reverb": "^1.5.1",
        "laravel/sanctum": "^4.1.1",
        "laravel/scout": "^10.15.0",
        "laravel/telescope": "^5.9.1",
        "laravel/tinker": "^2.10.1",
        "laravel/wayfinder": "^0.1.6",
        "league/flysystem-aws-s3-v3": "^3.29",
        "league/flysystem-read-only": "^3.28",
        "meilisearch/meilisearch-php": "^1.15.0",
        "pbmedia/laravel-ffmpeg": "^8.7.1",
        "pusher/pusher-php-server": "^7.2.7",
        "spatie/laravel-collection-macros": "^8.0",
        "spatie/laravel-data": "^4.17",
        "spatie/laravel-flash": "^1.10.1",
        "spatie/laravel-google-fonts": "^1.4.3",
        "spatie/laravel-medialibrary": "^11.13.0",
        "spatie/laravel-permission": "^6.20.0",
        "spatie/laravel-responsecache": "^7.7.0",
        "spatie/laravel-tags": "^4.10.0",
        "spatie/laravel-translatable": "^6.11.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5.5",
        "fakerphp/faker": "^1.24.1",
        "laravel/envoy": "^2.10.2",
        "laravel/pail": "^1.2.3",
        "laravel/pint": "^1.23.0",
        "laravel/sail": "^1.43.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "pestphp/pest": "^3.8.2",
        "pestphp/pest-plugin-laravel": "^3.2.0"
    },
    "autoload": {
        "files": [
            "src/Foundation/Helpers.php"
        ],
        "psr-4": {
            "App\\": "src/App/",
            "Domain\\": "src/Domain/",
            "Foundation\\": "src/Foundation/",
            "Support\\": "src/Support/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

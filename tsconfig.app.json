{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "resources/**/*",
        "resources/**/*.vue",
        "auto-imports.d.ts",
        "components.d.ts"
    ],
    "exclude": ["resources/**/__tests__/*"],
    "compilerOptions": {
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "paths": {
            "@/*": ["./resources/*"]
        }
    }
}
